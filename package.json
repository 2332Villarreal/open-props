{
  "type": "module",
  "name": "open-props",
  "author": "Adam Argyle",
  "license": "ISC",
  "version": "0.0.23",
  "repository": {
    "type": "git",
    "url": "https://github.com/argyleink/open-props"
  },
  "keywords": [
    "css",
    "utilities",
    "variables",
    "custom properties",
    "gradients"
  ],
  "style": "props.css",
  "source": "src/index.js",
  "main": "dist/open-props.cjs",
  "module": "./dist/open-props.module.js",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build": "npm run gen:op",
    "bundle": "concurrently npm:lib:*",
    "gen:op": "cd src && node _create-props.js '' true",
    "gen:nowhere": "cd src && node _create-props.js '' false",
    "gen:prefixed": "cd src && node _create-props.js 'op'",
    "lib:js": "microbundle",
    "lib:all": "postcss src/index.css -o props.css",
    "lib:animations": "postcss src/props.animations.css -o animations.css",
    "lib:aspects": "postcss src/props.aspects.css -o aspects.css",
    "lib:borders": "postcss src/props.borders.css -o borders.css",
    "lib:easing": "postcss src/props.easing.css -o easings.css",
    "lib:fonts": "postcss src/props.fonts.css -o fonts.css",
    "lib:gradients": "postcss src/props.gradients.css -o gradients.css",
    "lib:media": "postcss src/props.media.css -o media.css",
    "lib:shadows": "postcss src/props.shadows.css -o shadows.css",
    "lib:sizes": "postcss src/props.sizes.css -o sizes.css",
    "lib:supports": "postcss src/props.supports.css -o supports.css",
    "lib:zindex": "postcss src/props.zindex.css -o zindex.css",
    "lib:reset": "postcss src/extra/reset.css -o reset.css",
    "lib:colors": "postcss src/props.colors.css -o colors.css",
    "lib:colors:gray": "postcss src/props.gray.css -o gray.css",
    "lib:colors:red": "postcss src/props.red.css -o red.css",
    "lib:colors:pink": "postcss src/props.pink.css -o pink.css",
    "lib:colors:grape": "postcss src/props.grape.css -o grape.css",
    "lib:colors:violet": "postcss src/props.violet.css -o violet.css",
    "lib:colors:indigo": "postcss src/props.indigo.css -o indigo.css",
    "lib:colors:blue": "postcss src/props.blue.css -o blue.css",
    "lib:colors:cyan": "postcss src/props.cyan.css -o cyan.css",
    "lib:colors:teal": "postcss src/props.teal.css -o teal.css",
    "lib:colors:green": "postcss src/props.green.css -o green.css",
    "lib:colors:lime": "postcss src/props.lime.css -o lime.css",
    "lib:colors:yellow": "postcss src/props.yellow.css -o yellow.css",
    "lib:colors:orange": "postcss src/props.orange.css -o orange.css"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cssnano": "^5.0.8",
    "microbundle": "^0.13.3",
    "open-color": "^1.9.1",
    "postcss": "^8.3.9",
    "postcss-cli": "^8.3.1",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-easings": "3.x",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "6.7.x"
  }
}
